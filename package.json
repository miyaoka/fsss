{
  "name": "fsss",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm -r --parallel --if-present dev",
    "build": "pnpm -r --if-present build",
    "test:all": "pnpm -r --include-workspace-root --if-present --no-bail --aggregate-output --reporter=append-only test",
    "typecheck:all": "pnpm -r --include-workspace-root --if-present --no-bail --aggregate-output --reporter=append-only typecheck",
    "lint:all": "pnpm -r --include-workspace-root --if-present --no-bail --aggregate-output --reporter=append-only lint",
    "fix": "oxfmt '!apps/**' '!packages/**'",
    "example": "bun run apps/example/src/index.ts",
    "fix:all": "pnpm -r --include-workspace-root --if-present --no-bail --aggregate-output --reporter=append-only fix",
    "prepare": "lefthook install"
  },
  "devDependencies": {
    "@tsconfig/node24": "catalog:",
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:",
    "lefthook": "2.1.0",
    "oxfmt": "catalog:",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:",
    "typescript": "catalog:"
  }
}
