{
  "name": "@miyaoka/fsss",
  "version": "0.1.1",
  "description": "A CLI framework where your file structure becomes your command structure, and a single schema gives you typed values whether they come from flags, env vars, or config files.",
  "type": "module",
  "author": "miyaoka",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miyaoka/fsss.git"
  },
  "bugs": "https://github.com/miyaoka/fsss/issues",
  "keywords": [
    "cli",
    "framework",
    "file-based-routing",
    "zod",
    "bun",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "cp ../../README.md .",
    "postpack": "rm README.md",
    "prepare": "vite build",
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "bun test",
    "typecheck": "tsgo --noEmit",
    "lint": "oxlint --type-aware .",
    "fix": "oxlint --type-aware --fix . && oxfmt ."
  },
  "dependencies": {
    "zod": "catalog:"
  },
  "devDependencies": {
    "@tsconfig/node24": "catalog:",
    "@types/bun": "catalog:",
    "oxfmt": "catalog:",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:",
    "typescript": "catalog:",
    "unplugin-dts": "catalog:",
    "vite": "catalog:"
  }
}
